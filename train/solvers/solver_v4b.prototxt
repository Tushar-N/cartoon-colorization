net: "./models/colorization_v4.prototxt"
test_iter: 250 # number of iterations per step
test_interval: 100
test_initialization: False # don't run validation when loading
base_lr: 3.16e-5
lr_policy: "step"
gamma: 0.316
stepsize: 215000
display: 1
max_iter: 500000
momentum: 0.9
momentum2: 0.99
weight_decay: 0.001
snapshot: 100
snapshot_prefix: "./cv/v4b/v4b"
solver_mode: GPU
type: "Adam"
average_loss: 1000
